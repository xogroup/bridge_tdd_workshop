    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.23.1",
    "babel-istanbul": "^0.12.2",
